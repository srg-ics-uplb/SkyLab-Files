    Keywords = {LIFE sciences, CLOUD computing, COMPUTATIONAL grids (Computer systems), DATA analysis, WORKFLOW, WEB-based user interfaces, COMPUTER software, BIOINFORMATICS, Bioinformatics, high performance computing, Internet, workflows},
    Number = {1},
    Pages = {1 - 10},
    Title = {Yabi: An online research environment for grid, high performance and cloud computing.},
    Volume = {7},
